{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "PORT=3030 nodemon ./bin/www",
    "test": "jest --detectOpenHandles",
    "migrate": "sequelize db:migrate",
    "migrate-test": "NODE_ENV=test sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "coverage": "jest --detectOpenHandles --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "./models"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.9.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "postgres": "^1.0.2",
    "sequelize": "^6.5.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.6.3",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
